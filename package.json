{
    "name": "float",
    "version": "0.0.1",
    "description": "Discord bot",
    "main": "dist/main.js",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "tsup",
        "start": "node --enable-source-maps --experimental-specifier-resolution=node dist/main.js",
        "watch": "tsup --watch",
        "dev": "tsup --watch --onSuccess \"pnpm start\"",
        "typecheck": "tsc --noEmit",
        "lint": "eslint --ignore-path .eslintignore --ext .js,ts .",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "commit": "cz"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uwuLegacy/float.git"
    },
    "keywords": [
        "discord",
        "bot",
        "discord-bot"
    ],
    "author": "Legacy",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/uwuLegacy/float/issues"
    },
    "imports": {
        "#lib/*": "./dist/lib/*.js",
        "#config/*": "./dist/lib/config/*.js",
        "#util/*": "./dist/lib/util/*.js"
    },
    "homepage": "https://github.com/uwuLegacy/float#readme",
    "devDependencies": {
        "@swc/core": "^1.2.140",
        "@types/node": "^17.0.18",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.1",
        "tsup": "^5.11.13",
        "typescript": "^4.7.0-dev.20220215"
    },
    "dependencies": {
        "@discordjs/builders": "^0.12.0",
        "@ogma/common": "^1.0.0",
        "@ogma/logger": "^2.2.0",
        "@sapphire/decorators": "^4.2.4",
        "@sapphire/discord-utilities": "^2.8.0",
        "@sapphire/discord.js-utilities": "^4.8.0",
        "@sapphire/framework": "^3.0.0-next.1d89b7f.0",
        "@sapphire/pieces": "^3.2.0",
        "@sapphire/stopwatch": "^1.3.0",
        "@sapphire/utilities": "^3.3.0",
        "discord.js": "^13.6.0",
        "dotenv": "^16.0.0"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
